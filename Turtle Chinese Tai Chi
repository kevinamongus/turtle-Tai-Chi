import turtle as t
t.speed(0.51)
t.hideturtle()
t.circle(100,180)
t.circle(200,180)
t.begin_fill()
t.circle(100,-180)
t.left(180)
t.circle(100,180)
t.circle(200,-180)
t.end_fill()

t.penup()
t.goto(0,66)
t.pendown()
t.begin_fill()
t.circle(33)
t.end_fill()

t.penup()
t.goto(0,-66)
t.pendown()
t.fillcolor('white')
t.begin_fill()
t.circle(-33)
t.end_fill()
